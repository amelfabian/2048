<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:af="urn:import:be.steformation.af.client.ui.widget">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>

	<m:MaterialRow>
		<m:MaterialColumn>
			<m:MaterialRow> <!-- buttons -->
				<m:MaterialColumn grid="s2">
					<af:Down placeholder="Down" ui:field="down"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s2" offset="s4">
					<af:Up placeholder="Up" ui:field="up"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s2" offset="s6">
				 <af:Left placeholder="Left" ui:field="left"/>
				</m:MaterialColumn>
				<m:MaterialColumn grid="s2" offset="s8">
				<af:Right placeholder="Right" ui:field="right"/>
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialColumn>
	
	</m:MaterialRow>
</ui:UiBinder> 